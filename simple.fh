int a = 2;
int soma(int a, int b) do
  return a + b;
end

int square(int a) do
  int square = 0;
  int i = a;
  while (i > 0) do
    int j = a;
    while (j > 0) do
      square = square +1;
      j = j -1;
    end
    i = i - 1;
  end
  return square;
end

int main() do
  int result = soma(1, 2);
  int sqr = square(3);
  return result;
end